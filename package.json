{
  "name": "rentophila",
  "version": "1.0.0",
  "description": "rent everything here!",
  "main": "dist",
  "scripts": {
    "dev": "SET NODE_ENV=development & nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "SET NODE_ENV=production pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toshikverma/rentxx123.git"
  },
  "author": "Toshik Verma <phpdroidapps@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/toshikverma/rentxx123/issues"
  },
  "homepage": "https://github.com/toshikverma/rentxx123#readme",
  "dependencies": {
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "mongoose": "^4.12.6",
    "nodemailer": "^4.4.0",
    "random-token": "0.0.8"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.10.0"
  }
}
